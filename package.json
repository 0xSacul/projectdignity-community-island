{
  "name": "projectdignity-community-island",
  "description": "Project Dignity's Island for Sunflower Land.",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && esbuild ./src/Scene.ts --bundle --minify --tsconfig=tsconfig.json --outfile=public/Scene.js",
    "dev": "npm run clean && esbuild ./src/Scene.ts --bundle --minify --watch --tsconfig=tsconfig.json --outfile=public/Scene.js",
    "clean": "rimraf public/Scene.js"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "esbuild": "0.18.15",
    "phaser": "^3.60.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "rimraf": "^5.0.1"
  }
}
